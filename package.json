{
    "name": "tetris",
    "version": "1.0.0",
    "description": "A simple tetris game",
    "main": "game.ts",
    "homepage": "https://github.com/dawson-teu/TetrisClone/#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/dawson-teu/TetrisClone.git"
    },
    "scripts": {
        "lint-eslint": "echo Linting Typescript... && %cd%/node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx --fix --config .eslintrc.json --ignore-path .eslintignore . && echo Typescript linting succesful. No problems found. ",
        "lint-stylelint": "echo Linting CSS... && %cd%/node_modules/.bin/stylelint \"src/*.css\" \"src/*.html\" \"src/*.ts\" \"src/*.tsx\" \"src/*.js\" \"src/*.jsx\" && echo CSS linting succesful. No problems found. ",
        "lint": "npm run lint-eslint && npm run lint-stylelint",
        "build": "npm run count && npm run lint && echo Building... && %cd%/node_modules/.bin/webpack -p --config ./webpack.prod.js",
        "start": "npm run count && npm run lint && echo Starting... && %cd%/node_modules/.bin/webpack-dev-server --config ./webpack.dev.js",
        "count": "echo Significant Lines of Code Count: && cloc --exclude-list-file=.code ./src "
    },
    "author": "Dawson Teu",
    "license": "MIT",
    "dependencies": {
        "core-js": "^3.2.1",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@typescript-eslint/eslint-plugin": "^2.1.0",
        "@typescript-eslint/parser": "^2.1.0",
        "autoprefixer": "^9.6.1",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "colorguard": "^1.2.1",
        "css-loader": "^3.2.0",
        "eslint": "^6.3.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.2.0",
        "eslint-import-resolver-webpack": "^0.11.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "file-loader": "^4.2.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.8.0",
        "postcss-loader": "^3.0.0",
        "prettier": "1.18.2",
        "stylelint": "^10.1.0",
        "stylelint-config-prettier": "^5.2.0",
        "stylelint-config-standard": "^18.3.0",
        "stylelint-prettier": "^1.1.1",
        "ts-loader": "^6.0.4",
        "typescript": "^3.6.2",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.8",
        "webpack-dev-server": "^3.8.0",
        "webpack-merge": "^4.2.2"
    }
}
