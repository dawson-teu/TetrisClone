{
    "name": "tetris-clone",
    "version": "1.0.0",
    "description": "A clone of the video game Tetris. It runs in the browser on a website.",
    "main": "game.ts",
    "homepage": "https://github.com/dawson-teu/TetrisClone/#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/dawson-teu/TetrisClone.git"
    },
    "scripts": {
        "lint-eslint": "echo Linting Typescript... && eslint --ext .js,.jsx,.ts,.tsx --fix --config .eslintrc.json --ignore-path .eslintignore . && echo Typescript linting succesful. No problems found. ",
        "lint-stylelint": "stylelint \"src/**/*.css\" \"src/**/*.html\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\" --fix --formatter verbose",
        "lint": "npm run lint-eslint && npm run lint-stylelint",
        "prettier-check": "prettier --check \"src/**/*.ts",
        "build": "npm run count && npm run lint && npm run prettier-check && echo Building... && webpack -p --config ./webpack.prod.js",
        "dev-build": "echo Building... && webpack -p --config ./webpack.prod.js",
        "start": "echo Starting... && webpack-dev-server --config ./webpack.dev.js",
        "count": "echo Significant Lines of Code Count: && cloc --exclude-list-file=.code ./src "
    },
    "author": "Dawson Teu <dawsonteu@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "core-js": "^3.6.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@babel/plugin-transform-instanceof": "^7.8.3",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@types/react": "^16.9.32",
        "@types/react-dom": "^16.9.6",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "autoprefixer": "^9.7.5",
        "babel-loader": "^8.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "colorguard": "^1.2.1",
        "css-loader": "^6.7.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-import-resolver-webpack": "^0.12.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-array-func": "^3.1.4",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-promise": "^4.2.1",
        "file-loader": "^6.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.7.2",
        "postcss-loader": "^7.0.2",
        "postcss-preset-env": "^6.7.0",
        "prettier": "2.0.2",
        "stylelint": "^13.2.1",
        "stylelint-a11y": "^1.2.3",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
        "stylelint-group-selectors": "^1.0.7",
        "stylelint-high-performance-animation": "^1.5.0",
        "stylelint-images": "^2.0.1",
        "stylelint-prettier": "^1.1.2",
        "stylelint-selector-no-empty": "^1.0.7",
        "stylelint-selector-tag-no-without-class": "^2.0.3",
        "stylelint-value-no-unknown-custom-properties": "^2.0.0",
        "stylelint-z-index-value-constraint": "^1.1.0",
        "ts-loader": "^9.4.2",
        "typescript": "^3.8.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2"
    }
}
